{
  "name": "pipeline-consigliere",
  "description": "A rules-based GitLab pipeline linter",
  "version": "0.1.0",
  "homepage": "https://github.com/smurf667/pipeline-consigliere",
  "private": true,
  "license": "MIT",
  "bin": {
    "pipeline-consigliere": "bin/cli.mjs"
  },
  "scripts": {
    "lint": "eslint **/*.mjs",
    "prettier": "prettier **/*.mjs --check",
    "prettify": "prettier **/*.mjs --write",
    "test": "node test/tester.mjs"
  },
  "files": [
    "bin/**",
    "lib/**",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "ansi-colors": "4.1.3",
    "@gitbeaker/rest": "40.2.0",
    "prompts": "2.4.2",
    "yaml": "2.5.0"
  },
  "devDependencies": {
    "@eslint/js": "9.9.0",
    "eslint": "9.9.0",
    "eslint-config-prettier": "9.1.0",
    "globals": "15.9.0",
    "prettier": "3.3.3"
  }
}
